<h2>Examples</h2>
<ul id="examples">
	<li>
		<%=link_to image_tag('examples/icon-array-standard-incremental.jpeg'), "/pictographs/view?axis_font=Arial&axis_font_size=12&axis_labels=1&axis_position=left&cell_height=40&cell_spacing=5&cell_width=22&cols=10&icon=&legend_font=Arial&legend_font_size=12&risks_attributes%5B0%5D%5Bdescription%5D=out+of+100+people+don%27t+exhibit+this+property&risks_attributes%5B0%5D%5Bdisplay%5D=0&risks_attributes%5B0%5D%5Bhex%5D=%23DCDCDC&risks_attributes%5B1%5D%5Bdescription%5D=out+of+100+people+exhibit+this+property&risks_attributes%5B1%5D%5Bdisplay%5D=0&risks_attributes%5B1%5D%5Bhex%5D=%2364BEFF&risks_attributes%5B1%5D%5Bvalue%5D=12.0&risks_attributes%5B2%5D%5B_destroy%5D=0&risks_attributes%5B2%5D%5Bdescription%5D=&risks_attributes%5B2%5D%5Bdisplay%5D=0&risks_attributes%5B2%5D%5Bhex%5D=%230000FF&risks_attributes%5B2%5D%5Bvalue%5D=4.0&rows=10&title=&title_alignment=left&title_font=Helvetica&title_font_size=18" %>
